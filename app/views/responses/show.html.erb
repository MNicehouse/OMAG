 <div class="container-fluid">
   <div class="row flex-nowrap">
     <!--SIDEBAR -->
     <%# Booker %>
     <% if  (@role == 'User')%>
     <% content_for(:booker) { render partial: 'users/sidebar_client', locals: { myclient: @user  }} %>
     <%# Host %>
     <% elsif  (@role == 'Admin')%>
     <% content_for(:host) { render partial: 'users/sidebar_consultant', locals: { myconsultant: @user }} %>
     <% end%>
     <%= render  "users/navbar" %>
     <!--CONTENT -->
     <div class="col py-3">
       <%# Content area... %>
       <div class="container" style="margin-top: 12px">
          <h3>Hello, <%= @user.name%>!</h3>
          <p>You have totally <%= @questions.count %> questions to this test</p>
       </div>

       <!--CARDS -->
       <div class="container" style="margin-top: 12px">
         <%= render "question_to_response", myresponse: @response, myassessquestions: @questions, myquestionid: @myquestionid_new, myquestionindex: @myquestionindex_new %>
       </div>
     </div>
  </div>
</div>